{
  "manufacturer": "Umang",
  "keyboard_name": "Admin_pad",
  "maintainer": "Umang5848", // Your GitHub username
  "processor": "RP2040",
  "bootloader": "rp2040",
  "matrix_pins": {
    "rows": ["B1", "B2"], // GP10, GP11
    "cols": ["B3", "B4", "C0", "C1", "C2"] // GP12, GP13, GP14, GP15, GP16 (Enc Btn)
  },
  "diode_direction": "COL2ROW",
  "encoder": { // Define encoder pins for QMK metadata (optional but good practice)
    "pin_a": "C4", // GP18
    "pin_b": "C3", // GP17
    "resolution": 2
  },
  "usb": {
    "vid": "0xFEED", // Match config.h
    "pid": "0x0001", // Match config.h
    "device_version": "0.0.1"
  },
  "layouts": {
    "LAYOUT": {
      // Updated layout for 2x5 matrix
      "layout": [
        // Row 1
        {"x":0, "y":0, "label":"R1C1"}, {"x":1, "y":0, "label":"R1C2"}, {"x":2, "y":0, "label":"R1C3"}, {"x":3, "y":0, "label":"R1C4"}, {"x":4, "y":0, "label":"EncBtn"}, // Encoder Button at R1C5
        // Row 2
        {"x":0, "y":1, "label":"R2C1"}, {"x":1, "y":1, "label":"R2C2"}, {"x":2, "y":1, "label":"R2C3"}, {"x":3, "y":1, "label":"R2C4"}, {"x":4, "y":1, "label":"(NC)"} // Empty spot at R2C5
        // Optional: Add visual representation for encoder knob in VIAL
        , {"x":5, "y":0.5, "label":"Enc1", "encoder": true, "matrix": [null, 0]} // Link visual encoder to matrix index 0
      ]
    }
  },
  "features": {
        "bootmagic": false,
        "command": false,
        "console": false,
        "encoder": true, // Enable encoder feature flag
        "extrakey": true,
        "mousekey": false,
        "nkro": true
    },
   "vial": {
       "uid": "0xA0B1C2D3E4F50001", // Match config.h
       "unlock_combo": [[0,0], [0,4]] // R1C1 + R1C5 (Enc Btn)
    }
}
